{
  "schemaVersion": 1,
  "id": "disco",
  "version": "${version}",

  "name": "Discovery Mod",
  "description": "This is a mod used to understand fabric modding",
  "authors": [
    "Jim Pavan"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/disco/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.fabricmc.disco.DiscoveryMod"
    ]
  },
  "mixins": [
    "disco.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.17",
    "fabric-api": "*",
    "minecraft": "~1.19.4",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
